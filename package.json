{
  "name": "CampusGuide",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run tsc --",
    "clean": "rimraf artifacts",
    "lint": "tslint src/**/*.ts",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage",
    "tsc": "tsc"
  },
  "dependencies": {
    "http-status-codes": "^1.0.6",
    "moment": "^2.18.1",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.44.0",
    "react-native-action-button": "^2.6.9",
    "react-native-deprecated-custom-components": "^0.1.0",
    "react-native-device-info": "^0.10.2",
    "react-native-fs": "^2.3.2",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-maps": "^0.14.0",
    "react-native-scrollable-tab-view": "^0.7.2",
    "react-native-simple-store": "^1.2.0",
    "react-native-snackbar": "^0.3.6",
    "react-native-vector-icons": "^4.1.0",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "@types/react": "^15.0.28",
    "@types/react-native": "^0.44.10",
    "@types/react-redux": "^4.4.43",
    "concurrently": "^3.4.0",
    "empty": "^0.10.1",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-react-native": "^18.0.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "2.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "moduleNameMapper": {
      "^[./]*assets[a-zA-Z0-9./]*/([a-zA-Z.]+)$": "empty/object"
    },
    "preset": "react-native"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
