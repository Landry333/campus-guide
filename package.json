  "scripts": {
    "flow": "flow .",
    "lint": "eslint .",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage"
  },
  "dependencies": {
    "http-status-codes": "^1.0.6",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-native": "^0.40.0",
    "react-native-action-button": "^2.0.20",
    "react-native-device-info": "^0.9.9",
    "react-native-fs": "^2.1.0-rc.1",
    "react-native-keyboard-aware-scroll-view": "^0.2.6",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-maps": "^0.13.0",
    "react-native-scrollable-tab-view": "^0.7.0",
    "react-native-simple-store": "^1.1.0",
    "react-native-vector-icons": "^4.0.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-native": "^1.9.1",
    "empty": "^0.10.1",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-react-native": "^2.2.0",
    "flow-bin": "^0.36.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "jest-react-native": "^18.0.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^15.4.0-rc.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "moduleNameMapper": {
      "^[./]*assets[a-zA-Z0-9./]*/([a-zA-Z.]+)$": "empty/object"
    },
    "preset": "react-native"
  },
  "pre-commit": [
    "flow",
    "lint",
    "test"
  ]
}
